{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["id", "type", "timestamp", "command", "payload"],
  "properties": {
    "command": {"const": "file/accept_ack"},
    "payload": {
      "type": "object",
      "required": ["status"],
      "properties": {
        "status": {"type": "integer"},
        "session_id": {"type": "string"},
        "error_message": {"type": "string"}
      }
    }
  }
}
